\begin_layout Standard
In the following, let 
\begin_inset Formula $\Delta\mathcal{F}\in\mathbb{R}^{m}$
\end_inset

 denote the quantity
\begin_inset Formula 
\[
\Delta\mathcal{F}=\left(\Delta\mathcal{F}_{\rho v},\Delta\mathcal{F}_{\rho e_{t}},\Delta\mathcal{F}_{C_{1}},\ldots,\Delta\mathcal{F}_{C_{n_{s}}}\right)^{T},
\]

\end_inset

where 
\begin_inset Formula $\Delta\mathcal{F}_{\rho v}\in\mathbb{R}^{d}$
\end_inset

 and the remaining entries are in 
\begin_inset Formula $\mathbb{R}$
\end_inset

.
 The proof below is similar to that in
\begin_inset space ~
\end_inset


\begin_inset CommandInset citation
LatexCommand citep
after "Lemma 6"
key "Zha17"
literal "false"

\end_inset

.
\end_layout

\begin_layout Lemma
\begin_inset CommandInset label
LatexCommand label
name "lem:alpha-constraints"

\end_inset

Assume that 
\begin_inset Formula $y=\left(\rho v,\rho e_{t},C_{1},\ldots,C_{n_{s}}\right)^{T}$
\end_inset

 is in 
\begin_inset Formula $\mathcal{G}$
\end_inset

.
 Then 
\begin_inset Formula $\check{y}=y-\alpha^{-1}\Delta\mathcal{F}$
\end_inset

, where 
\begin_inset Formula $\alpha>0$
\end_inset

, is also in 
\begin_inset Formula $\mathcal{G}$
\end_inset

 under the following conditions:
\end_layout

\begin_layout Lemma
\begin_inset Formula 
\begin{equation}
\alpha>\alpha^{*}\left(y,\Delta\mathcal{F}\right)=\max\left\{ \max_{i=1,\ldots,n_{s}}\frac{\mathcal{F}_{C_{i}}\cdot n}{C_{i}},\left.\alpha_{T}\right|_{\left(y,\Delta\mathcal{F}\right)},0\right\} ,\label{eq:alpha-constraint}
\end{equation}

\end_inset

where
\begin_inset Formula 
\begin{equation}
\alpha_{T}=\begin{cases}
\frac{-\mathsf{b}+\sqrt{\mathsf{b}^{2}-4\rho^{2}u\mathsf{g}}}{2\rho^{2}u}, & \mathsf{b}^{2}-4\rho^{2}u\mathsf{g}\geq0\\
0, & \mathrm{otherwise}
\end{cases},\label{eq:alpha_T}
\end{equation}

\end_inset


\begin_inset Formula $\mathsf{b}=-\rho e_{t}\mathsf{M}-\rho\Delta\mathcal{F}_{\rho e_{t}}+\rho v\cdot\Delta\mathcal{F}_{\rho v}+2\rho u_{0}\mathsf{M}$
\end_inset

, and 
\begin_inset Formula $\mathsf{g}=\mathsf{M}\Delta\mathcal{F}_{\rho e_{t}}-\frac{1}{2}\left|\Delta\mathcal{F}_{\rho v}\right|^{2}-u_{0}\mathsf{M}^{2}$
\end_inset

, and 
\begin_inset Formula $\mathsf{M}=\sum_{i=1}^{n_{s}}W_{i}\Delta\mathcal{F}_{C_{i}}$
\end_inset

.
\end_layout

\begin_layout Proof
\begin_inset Formula $\check{y}=y-\alpha^{-1}\Delta\mathcal{F}$
\end_inset

 can be expanded as
\begin_inset Formula 
\[
\begin{split}\check{y}= & \left(\check{\rho v},\check{\rho e_{t}},\check{C_{1}},\ldots,\check{C}_{n_{s}}\right)^{T}\\
= & \left(\rho v-\alpha^{-1}\Delta\mathcal{F}_{\rho v},\rho e_{t}-\alpha^{-1}\Delta\mathcal{F}_{\rho e_{t}},C_{1}-\alpha^{-1}\Delta\mathcal{F}_{C_{1}},\ldots,C_{n_{s}}-\alpha^{-1}\Delta\mathcal{F}_{C_{n_{s}}}\right)^{T}.
\end{split}
\]

\end_inset

First, we focus on positivity of density and species concentrations.
 For the 
\begin_inset Formula $i$
\end_inset

th species, 
\begin_inset Formula $C_{i}-\alpha^{-1}\mathcal{F}_{C_{i}}\cdot n>0$
\end_inset

 if 
\begin_inset Formula $\alpha>\max\left\{ \left(\mathcal{F}_{C_{i}}\cdot n\right)/C_{i},0\right\} $
\end_inset

.
 Accounting for all species yields
\begin_inset Formula 
\[
\alpha>\max\left\{ \max_{i=1,\ldots,n_{s}}\frac{\mathcal{F}_{C_{i}}\cdot n}{C_{i}},0\right\} .
\]

\end_inset

Density is then also positive.
 
\end_layout

\begin_layout Proof
Next, we focus on positivity of temperature.
 For a given 
\begin_inset Formula $y=\left(\rho v,\rho e_{t},C_{1},\ldots,C_{n_{s}}\right)^{T}$
\end_inset

, let 
\begin_inset Formula $Z(y)$
\end_inset

 be defined as
\begin_inset Formula 
\begin{equation}
Z(y)=\rho^{2}u^{*}(y)=\rho(y)\rho e_{t}-\left|\rho v\right|^{2}/2-\rho^{2}u_{0}(y).\label{eq:Z-definition}
\end{equation}

\end_inset

Note that if 
\begin_inset Formula $Z(y)>0$
\end_inset

, then 
\begin_inset Formula $T(y)>0$
\end_inset

.
 
\begin_inset Formula $Z\left(\check{y}\right)$
\end_inset

 can be expressed as
\begin_inset Formula 
\begin{align*}
Z\left(y-\alpha^{-1}\Delta\mathcal{F}\right)= & \sum_{i=1}^{n_{s}}W_{i}\left(C_{i}-\alpha^{-1}\Delta\mathcal{F}_{C_{i}}\right)\left(\rho e_{t}-\alpha^{-1}\Delta\mathcal{F}_{\rho e_{t}}\right)\\
 & -\frac{1}{2}\left|\rho v-\alpha^{-1}\Delta\mathcal{F}_{\rho v}\right|^{2}-\left[\sum_{i=1}^{n_{s}}W_{i}\left(C_{i}-\alpha^{-1}\Delta\mathcal{F}_{C_{i}}\right)\right]^{2}u_{0},
\end{align*}

\end_inset

which, after multiplying both sides by 
\begin_inset Formula $\alpha^{2}$
\end_inset

, can be rewritten as 
\begin_inset Formula 
\begin{align}
\alpha^{2}Z\left(y-\alpha^{-1}\Delta\mathcal{F}\right)= & \rho^{2}u\alpha^{2}-\mathsf{b}\alpha+\mathsf{g},\label{eq:alpha-quadratic-form}
\end{align}

\end_inset

where 
\begin_inset Formula $\mathsf{b}=-\rho e_{t}\mathsf{M}-\rho\Delta\mathcal{F}_{\rho e_{t}}+\rho v\cdot\Delta\mathcal{F}_{\rho v}+2\rho u_{0}\mathsf{M}$
\end_inset

, 
\begin_inset Formula $\mathsf{g}=\mathsf{M}\Delta\mathcal{F}_{\rho e_{t}}-\frac{1}{2}\left|\Delta\mathcal{F}_{\rho v}\right|^{2}-u_{0}\mathsf{M}^{2}$
\end_inset

, and 
\begin_inset Formula $\mathsf{M}=\sum_{i=1}^{n_{s}}W_{i}\Delta\mathcal{F}_{C_{i}}$
\end_inset

.
 Setting the RHS of Equation
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:alpha-quadratic-form"
plural "false"
caps "false"
noprefix "false"

\end_inset

 equal to zero yields a quadratic equation with 
\begin_inset Formula $\alpha$
\end_inset

 as the unknown.
 Since 
\begin_inset Formula $\rho^{2}u$
\end_inset

 is positive, the quadratic equation is convex.
 As such, if 
\begin_inset Formula $\mathsf{b}^{2}-4\rho^{2}u\mathsf{g}<0$
\end_inset

, then no real roots exist, and 
\begin_inset Formula $Z\left(y-\alpha^{-1}\Delta\mathcal{F}\right)>0$
\end_inset

 for all 
\begin_inset Formula $\alpha\neq0$
\end_inset

; otherwise, at least one real root exists, in which case a sufficient condition
 to ensure 
\begin_inset Formula $Z\left(y-\alpha^{-1}\Delta\mathcal{F}\right)>0$
\end_inset

 is 
\begin_inset Formula $\alpha>\alpha_{0}$
\end_inset

, where 
\begin_inset Formula $\alpha_{0}$
\end_inset

 is given by
\begin_inset Formula 
\[
\alpha_{0}=\max\left\{ \frac{-\mathsf{b}+\sqrt{\mathsf{b}^{2}-4\rho^{2}u\mathsf{g}}}{2\rho^{2}u},0\right\} .
\]

\end_inset


\end_layout

\begin_layout Remark
\begin_inset CommandInset label
LatexCommand label
name "rem:simpler-alpha-constraint"

\end_inset

If 
\begin_inset Formula $\Delta\mathcal{F}_{C_{i}}=0,$
\end_inset

 for all 
\begin_inset Formula $i$
\end_inset

, then 
\begin_inset Formula $\alpha^{*}$
\end_inset

 in 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:alpha-constraint"
plural "false"
caps "false"
noprefix "false"

\end_inset

can be simplified to 
\begin_inset Formula 
\[
\alpha^{*}\left(y,\Delta\mathcal{F}\right)=\max\left\{ \left.\alpha_{T}\right|_{\left(y,\Delta\mathcal{F}\right)},0\right\} .
\]

\end_inset


\end_layout

