\begin_inset Formula 
\begin{align}
\overline{y}_{\kappa} & =\sum_{v=1}^{n_{q}}\frac{\left|J_{\kappa}(\xi_{v})\right|w_{v}}{|\kappa|}y_{\kappa}\left(\xi_{v}\right),\nonumber \\
 & =\sum_{v=1}^{n_{q}}\theta_{v}y_{\kappa}\left(\xi_{v}\right)+\sum_{f=1}^{n_{f}}\sum_{l=1}^{n_{q,f}^{\partial}}\theta_{f,l}y_{\kappa}\left(\xi\left(\zeta_{l}^{(f)}\right)\right).\label{eq:element-average-convex-combination-2D}
