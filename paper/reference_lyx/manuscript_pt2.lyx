\begin_layout Subsubsection
Limiting procedure
\begin_inset CommandInset label
LatexCommand label
name "subsec:limiting-procedure"

\end_inset


\end_layout

\begin_layout Standard
The limiting procedure to enforce 
\begin_inset Formula $y_{\kappa}^{j+1}(x)\in\mathcal{G}_{s_{b}},\:\forall x\in\mathcal{D_{\kappa}}$
\end_inset

, is the same as in Part I.
 For completeness, we summarize it here.
 The 
\begin_inset Formula $j$
\end_inset

+1 superscript and 
\begin_inset Formula $\kappa$
\end_inset

 subscript are dropped for brevity.
 
\begin_inset Formula $\overline{y}_{\kappa}^{j}(x)$
\end_inset

 is assumed to be in 
\begin_inset Formula $\mathcal{G}_{s_{b}}$
\end_inset

.
 The limiting operator is of the same form as in 
\begin_inset CommandInset citation
LatexCommand citep
key "Wan12"
literal "false"

\end_inset

, 
\begin_inset CommandInset citation
LatexCommand citep
key "Zha17"
literal "false"

\end_inset

, 
\begin_inset CommandInset citation
LatexCommand citep
key "Jia18"
literal "false"

\end_inset

, 
\begin_inset CommandInset citation
LatexCommand citep
key "Wu21_2"
literal "false"

\end_inset

, and related papers.
\end_layout

\begin_layout Enumerate
Positive density: if 
\begin_inset Formula $\rho(x)>\epsilon,\:\forall x\in\mathcal{D}_{\kappa}$
\end_inset

, where 
\begin_inset Formula $\epsilon>0$
\end_inset

 is a small number (e.g., 
\begin_inset Formula $\epsilon=10^{-10}$
\end_inset

), then set 
\begin_inset Formula $C_{i}^{(1)}=C_{i}=\sum_{j=1}^{n_{b}}C_{i}(x_{j})\phi_{j},i=1,\ldots,n_{s}$
\end_inset

; otherwise, compute
\begin_inset Formula 
\[
C_{i}^{(1)}=\overline{C}_{i}+\theta^{(1)}\left(C_{i}-\overline{C}_{i}\right),\quad\theta^{(1)}=\frac{\rho(\overline{y})-\epsilon}{\rho(\overline{y})-\underset{x\in\mathcal{D}}{\min}\rho(y(x))}.
\]

\end_inset

for 
\begin_inset Formula $i=1,\ldots,n_{s}.$
\end_inset


\end_layout

\begin_layout Enumerate
Nonnegative concentrations: if 
\begin_inset Formula $C_{i}^{(1)}(x)\geq0,\:\forall x\in\mathcal{D}_{\kappa}$
\end_inset

, then set 
\begin_inset Formula $C_{i}^{(2)}=C_{i}^{(1)},i=1,\ldots,n_{s}$
\end_inset

; otherwise, compute
\begin_inset Formula 
\[
C_{i}^{(2)}=\overline{C}_{i}+\theta^{(2)}\left(C_{i}^{(1)}-\overline{C}_{i}\right),\quad\theta^{(2)}=\frac{\overline{C}_{i}}{\overline{C}_{i}-\underset{x\in\mathcal{D}}{\min}C_{i}^{(1)}(x)}.
\]

\end_inset

Let 
\begin_inset Formula $y^{(2)}=\left(\rho v_{1},\ldots,\rho v_{d},\rho e_{t},C_{1}^{(2)},\ldots,C_{n_{s}}^{(2)}\right)$
\end_inset

.
\end_layout

\begin_layout Enumerate
Positive temperature: if 
\begin_inset Formula $\rho u^{*}\left(y^{(2)}(x)\right)>\epsilon,\:\forall x\in\mathcal{D}_{\kappa}$
\end_inset

, then set 
\begin_inset Formula $y^{(3)}=y^{(2)}$
\end_inset

; otherwise, compute
\begin_inset Formula 
\[
y^{(3)}=\overline{y}+\theta^{(3)}\left(y^{(2)}-\overline{y}\right),\quad\theta^{(3)}=\frac{\rho u^{*}(\overline{y})-\epsilon}{\rho u^{*}(\overline{y})-\underset{x\in\mathcal{D}}{\min}\rho u^{*}(y^{(2)}(x))}.
\]

\end_inset

The 
\begin_inset Quotes eld
\end_inset

positivity-preserving limiter
\begin_inset Quotes erd
\end_inset

 refers to the limiting procedure up to this point.
 
\end_layout

\begin_layout Enumerate
Entropy constraint: if 
\begin_inset Formula $\chi\left(y^{(3)}(x)\right)\geq0,\:\forall x\in\mathcal{D}_{\kappa}$
\end_inset

, then set 
\begin_inset Formula $y^{(4)}=y^{(3)}$
\end_inset

; otherwise, compute
\begin_inset Formula 
\[
y^{(4)}=\overline{y}+\theta^{(4)}\left(y^{(3)}-\overline{y}\right),\quad\theta^{(4)}=\frac{\chi(\overline{y})}{\chi(\overline{y})-\underset{x\in\mathcal{D}}{\min}\chi(y^{(3)}(x))}.
\]

\end_inset

The 
\begin_inset Quotes eld
\end_inset

entropy limiter
\begin_inset Quotes erd
\end_inset

 refers to the limiting procedure up to this point.
\end_layout

\begin_layout Standard
\begin_inset Formula $y^{(4)}$
\end_inset

 then replaces 
\begin_inset Formula $y$
\end_inset

 as the solution.
 This limiting procedure is applied at the end of every RK stage.
 It is conservative and in general preserves the formal order of accuracy
 for smooth solutions
\begin_inset space ~
\end_inset


\begin_inset CommandInset citation
LatexCommand citep
key "Zha10,Zha17,Zha12_2,Lv15_2,Jia18"
literal "false"

\end_inset

.
\end_layout

